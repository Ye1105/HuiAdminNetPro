{
  "signin": {
    "type": "object",
    "description": "手机号密码登录参数规则",
    "properties": {
      "phone": {
        "type": "string",
        "title": "手机",
        "pattern": "^1[3456789]\\d{9}$"
      },
      "password": {
        "type": "string",
        "title": "密码",
        "pattern": "^[\\S]{6,12}$"
      }
    },
    "required": [
      "phone",
      "password"
    ]
  },
  "account-create": {
    "type": "object",
    "description": "账号创建参数规则",
    "properties": {
      "name": {
        "type": "string",
        "title": "用户名",
        "minLength": 1
      },
      "phone": {
        "type": "string",
        "title": "手机号",
        "pattern": "^1[3456789]\\d{9}$"
      },
      "password": {
        "type": "string",
        "title": "密码",
        "minLength": 6,
        "maxLength": 12
      },
      "sex": {
        "type": "string",
        "title": "性别",
        "enum": [
          "男性",
          "女性",
          "跨性别"
        ]
      },
      "jobStatus": {
        "type": "string",
        "title": "工作状态",
        "enum": [
          "在职",
          "离职",
          "出差",
          "休假"
        ]
      }
    },
    "required": [
      "name",
      "phone",
      "password",
      "sex",
      "jobStatus"
    ]
  },
  "account-put": {
    "type": "object",
    "description": "账号 put 参数规则",
    "properties": {
      "uId": {
        "type": "string",
        "title": "用户Id"
      },
      "name": {
        "type": "string",
        "title": "用户名",
        "minLength": 1
      },
      "phone": {
        "type": "string",
        "title": "手机号",
        "pattern": "^1[3456789]\\d{9}$"
      },
      "sex": {
        "type": "string",
        "title": "性别",
        "enum": [
          "男性",
          "女性",
          "跨性别"
        ]
      },
      "jobStatus": {
        "type": "string",
        "title": "工作状态",
        "enum": [
          "在职",
          "离职",
          "出差",
          "休假"
        ]
      },
      "status": {
        "type": "string",
        "title": "账号状态",
        "enum": [
          "启用",
          "禁用"
        ]
      }
    },
    "required": [
      "uId",
      "name",
      "phone",
      "sex",
      "jobStatus",
      "status"
    ]
  },
  "account-patch": {
    "type": "object",
    "description": "账号 patch 参数规则",
    "properties": {
      "method": {
        "type": "string",
        "title": "方式类型",
        "enum": [
          "解锁",
          "锁定",
          "修改密码"
        ]
      },
      "uIds": {
        "type": "array",
        "title": "uId列表",
        "items": {
          "type": "string"
        }
      },
      "password": {
        "type": "string",
        "title": "密码"
      }
    },
    "required": [
      "method"
    ]
  },
  "account-psd": {
    "type": "object",
    "description": "账号 修改密码 参数规则",
    "properties": {
      "uId": {
        "type": "string",
        "title": "用户Id"
      },
      "password": {
        "type": "string",
        "title": "密码",
        "minLength": 6,
        "maxLength": 12
      },
      "cpassword": {
        "type": "string",
        "title": "确认密码",
        "minLength": 6,
        "maxLength": 12
      }
    },
    "required": [
      "uId",
      "password"
    ]
  },
  "role-create": {
    "type": "object",
    "description": "role创建规则",
    "properties": {
      "name": {
        "type": "string",
        "title": "角色名",
        "minLength": 1
      }
    },
    "required": [
      "name"
    ]
  },
  "role-edit": {
    "type": "object",
    "description": "role编辑规则",
    "properties": {
      "rId": {
        "type": "string",
        "title": "角色Id"
      },
      "name": {
        "type": "string",
        "title": "角色名",
        "minLength": 1
      }
    },
    "required": [
      "rId",
      "name"
    ]
  },
  "route-create": {
    "type": "object",
    "description": "路由编辑规则",
    "properties": {
      "pId": {
        "type": "string",
        "title": "父节点"
      },
      "route": {
        "type": "string",
        "title": "路由",
        "pattern": "(^\/[a-z]{1,15}$)|(^\/{1}$)",
        "minLength": 1
      },
      "name": {
        "type": "string",
        "title": "名称",
        "pattern": "^[\\u4e00-\\u9fa5]*$",
        "minLength": 1
      },
      "order": {
        "type": "number",
        "title": "排序",
        "minimum": 0
      },
      "status": {
        "type": "string",
        "title": "状态",
        "enum": [
          "启用",
          "禁用"
        ]
      }
    },
    "required": [
      "pId",
      "route",
      "name",
      "order",
      "status"
    ]
  }
}